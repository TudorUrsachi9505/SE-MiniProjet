if (solve(labyrinth, x+1, y, m, n) or solve(labyrinth, x-1, y, m, n) or solve(labyrinth, x, y+1, m, n) or solve(labyrinth, x, y-1, m, n)):
                if labyrinth[y][x] == visited:
                    labyrinth[y][x] = good
                path = True
        elif labyrinth[y][x] == end:
            path = True
    return path
    
